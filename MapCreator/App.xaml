<Application x:Class="MapCreator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MapCreator"
             xmlns:converter="clr-namespace:MapCreator.Converter"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converter:BitmapToImageSourceConverter x:Key="BitmapConverter"/>
        <converter:TextureToWritableBitMap x:Key="TextureConverter"/>
    </Application.Resources>
</Application>
